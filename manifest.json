{
    "name": "Notty Notes Local"
    ,"version":"1.1"
    ,"manifest_version":2
    ,"description":"One of The Best Sticky Notes For Your Broswer"

    ,"app": {
        "launch": {
            "local_path": "main.html"
        }
    }
    ,"icons":{
        "16":"assets/32.png"
        ,"32":"assets/32.png"
        ,"48":"assets/48.png"
        ,"128":"assets/128.png"
        ,"256":"assets/256.png"
    }

   ,"permissions": [
       "unlimitedStorage"
       ,"contextMenus"
       ,"tabs"
   ]
}
